<Window x:Class="Nemo.supervisor.supervisor"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Nemo.supervisor"
        mc:Ignorable="d"
        Title="supervisor" Height="450" Width="800">
    <Window.Background>
        <ImageBrush ImageSource="emoji-background.png" TileMode="Tile"/>
    </Window.Background>
    <Border Margin="20">
        <Grid>
            <Grid.ColumnDefinitions>
                <ColumnDefinition Width="200"/>
                <ColumnDefinition Width="489*"/>
            </Grid.ColumnDefinitions>



            <StackPanel Grid.Column="0">



            </StackPanel>
            <Line X1="200" Y1="0" X2="200" Y2="350"
                   Stroke="Black"/>
            <Frame Grid.Column="1" x:Name="supervisorframe" Grid.ColumnSpan="2" Margin="0,0,-0.4,0">
            </Frame>

        </Grid>


    </Border>
</Window>
